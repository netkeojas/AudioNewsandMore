<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.newsandmore">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AudioPlayer"></activity>
        <activity android:name=".Feedback" />
        <activity android:name=".ListOfBooks" />
        <activity android:name=".Books" />
        <activity android:name=".ListOfNews" />
        <activity android:name=".News" />
        <activity android:name=".ListOfArt" />
        <activity android:name=".Articles" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>